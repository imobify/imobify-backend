{
  "name": "imobify-backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "Gabriel Lima",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "cz": "cz",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 3 && npm run prisma:dev:deploy",
    "db:dev:rm": "docker compose rm dev-db -s -f",
    "db:dev:up": "docker compose up dev-db -d",
    "db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 3 && npm run prisma:test:deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "eslint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "lint": "npm run eslint:check && npm run prettier:check",
    "precommit": "lint-staged && npm run lint",
    "prepare": "husky install",
    "prepush": "npm run test",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "prisma:dev:deploy": "prisma migrate deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@nestjs/common": "10.2.1",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.2.1",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "10.0.1",
    "@nestjs/platform-express": "10.2.1",
    "@prisma/client": "5.2.0",
    "argon2": "0.31.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cloudinary": "1.40.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "streamifier": "0.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@nestjs/cli": "10.1.13",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.4",
    "@types/multer": "1.4.7",
    "@types/node": "20.5.3",
    "@types/passport-jwt": "3.0.9",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv-cli": "7.3.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.1",
    "husky": "8.0.3",
    "jest": "29.6.3",
    "lint-staged": "13.3.0",
    "pactum": "3.5.0",
    "prettier": "2.8.8",
    "prisma": "5.2.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "optionalDependencies": {
    "fsevents": "2.3.3"
  },
  "engines": {
    "node": "^16.14.0 || ^18.10.0 || ^20.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
